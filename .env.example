# Google Custom Search API Configuration
SEARCH_API_KEY=your_google_custom_search_api_key
SEARCH_CX=your_custom_search_engine_id
# Optional Proxy Configuration
SEARCH_PROXY=http://proxy_server:port

# Ollama configuration
OLLAMA_HOST=0.0.0.0
OLLAMA_PORT=11434
SUPPORTED_MODELS=llama3.1,mistral,qwen2.5
DEFAULT_MODEL=llama3.1

# MCP Server Configuration
MCP_SERVER_HOST=0.0.0.0
MCP_SERVER_PORT=8765
# Workspace configuration
WORKSPACE_PATH=/tmp/agent_workspace

# Redis configuration
REDIS_HOST=localhost
REDIS_PORT=6379
SESSION_EXPIRY=3600

# Security configuration
ENABLE_DOCKER_SANDBOX=true
REQUIRE_PERMISSIONS=true

# API configuration
API_PORT=8000
API_HOST=0.0.0.0

# Debug mode
DEBUG=false

# Logging configuration
LOG_LEVEL=INFO